{
   "name": "api-test",
   "version": "1.0.0",
   "description": "",
   "main": "app.ts",
   "scripts": {
      "start:dev": "cross-env NODE_ENV=development yarn build && cross-env NODE_ENV=development nodemon build/src/server.js",
      "build": "rimraf ./build && yarn tsoa:gen && tsc && mkdirp .tmp && yarn cp:configs",
      "cp:configs": "cpx \"./src/config/*.json\" \"./build/src/config\"",
      "tsoa:gen": "tsoa spec-and-routes && tsc --outDir build --experimentalDecorators",
      "pretest": "cross-env NODE_ENV=test yarn build",
      "test": "cross-env NODE_ENV=test tsc && cross-env NODE_ENV=test jest --collectCoverage --runInBand --passWithNoTests",
      "test:debug:osx": "node --inspect node_modules/.bin/jest --runInBand",
      "test:debug:windows": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
      "test:verbose": "cross-env LOG_LEVEL=debug PRETTY_PRINT_LOGS=true yarn test",
      "typeorm": "typeorm-ts-node-esm",
      "add-migration": "cross-env NODE_ENV=development yarn build && typeorm migration:generate",
		"update-database": "yarn run cross-env NODE_ENV=development typeorm migration:run",
		"revert-database": "cross-env NODE_ENV=development typeorm migration:revert",
      "clean-migrations": "rimraf ./src/infra/migrations/"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@koa/router": "^12.0.0",
      "@types/axios": "^0.14.0",
      "@types/body-parser": "^1.19.2",
      "@types/express": "^4.17.13",
      "@types/kcors": "^2.2.6",
      "@types/koa": "^2.13.5",
      "@types/koa-bodyparser": "^4.3.7",
      "@types/koa-json": "^2.0.20",
      "@types/koa-logger": "^3.1.2",
      "@types/koa-router": "^7.4.4",
      "@types/koa-static": "^4.0.2",
      "@types/koa__router": "^12.0.0",
      "@types/morgan": "^1.9.3",
      "@types/node": "^18.7.18",
      "axios": "^0.27.2",
      "body-parser": "^1.20.0",
      "cors": "^2.8.5",
      "cpx": "^1.5.0",
      "cross-env": "^7.0.3",
      "dotenv": "^16.0.2",
      "express": "^4.18.1",
      "jest": "^29.0.3",
      "kcors": "^2.2.2",
      "koa": "^2.13.4",
      "koa-bodyparser": "^4.3.0",
